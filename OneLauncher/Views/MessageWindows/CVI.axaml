<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="250"
        x:Class="OneLauncher.CVI"
        Title="填写版本信息"
		TransparencyLevelHint="AcrylicBlur"
        Background="#B2FFFFFF"
        ExtendClientAreaToDecorationsHint="True"
		Width="200"
		Height="250"
		>
	<Panel>
	    <StackPanel>
            <TextBlock Text="收集信息..." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,12,0,0" FontFamily="avares://OneLauncher/Assets/blod.otf#思源黑体"/>
			<TextBox x:Name="SomeInfomations" Watermark="输入..." Margin="12"/>
			<Button Content="下一步" Margin="12" Click="Button_Click"/>
	    </StackPanel>
	</Panel>
</Window>
